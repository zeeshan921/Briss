

{%- liquid 
  assign padding_horizontal = section.settings.padding_horizontal
  assign padding_horizontal_mobile = section.settings.padding_horizontal_mobile
  assign padding_top = section.settings.padding_top
  assign padding_bottom = section.settings.padding_bottom
  assign border_color = section.settings.border_color
  assign border_thickness = section.settings.border_thickness
  assign margin_top = section.settings.margin_top
  assign margin_bottom = section.settings.margin_bottom
  assign background_color = section.settings.background_color
  assign background_gradient = section.settings.background_gradient
  assign lazy = section.settings.lazy

  assign tabs_position = section.settings.tabs_position

  assign tab_padding_vertical_mobile = section.settings.tab_padding_vertical_mobile
  assign tab_padding_vertical = section.settings.tab_padding_vertical
  assign tab_padding_horizontal_mobile = section.settings.tab_padding_horizontal_mobile
  assign tab_padding_horizontal = section.settings.tab_padding_horizontal
  assign tab_radius = section.settings.tab_radius
  assign tab_border_thickness = section.settings.tab_border_thickness
  assign tab_border_color = section.settings.tab_border_color
  assign tab_bg_color = section.settings.tab_bg_color
  assign tab_mt = section.settings.tab_mt
  assign tab_mt_mobile = section.settings.tab_mt_mobile  

  assign title_icon_size = section.settings.title_icon_size
  assign title_icon_size_mobile = section.settings.title_icon_size_mobile
  assign title_icon_color = section.settings.title_icon_color

  assign title_size = section.settings.title_size
  assign title_size_mobile = section.settings.title_size_mobile
  assign title_color = section.settings.title_color
  assign title_custom = section.settings.title_custom
  assign title_font = section.settings.title_font
  assign title_height = section.settings.title_height
  assign title_gap = section.settings.title_gap
  assign title_gap_mobile = section.settings.title_gap_mobile
  
  assign text_size = section.settings.text_size
  assign text_size_mobile = section.settings.text_size_mobile
  assign text_color = section.settings.text_color
  assign text_custom = section.settings.text_custom
  assign text_font = section.settings.text_font
  assign text_height = section.settings.text_height
  assign text_padding_top = section.settings.text_padding_top

  assign arrow_color = section.settings.arrow_color
  assign arrow_size = section.settings.arrow_size
  assign arrow_size_mobile = section.settings.arrow_size_mobile
-%}

{%- style -%}
  {{  title_font | font_face: font_display: 'swap' }}
  {{  text_font | font_face: font_display: 'swap' }}

  .section-{{ section.id }} {
    border-top: solid {{ border_color }} {{ border_thickness }}px;
    border-bottom: solid {{ border_color }} {{ border_thickness }}px;
    margin-top: {{ margin_top | times: 0.75 | round: 0 }}px !important;
    margin-bottom: {{ margin_bottom | times: 0.75 | round: 0 }}px !important;
  }
  
  .section-{{ section.id }}-settings {
    margin: 0 auto;
    padding-top: {{ padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ padding_bottom | times: 0.75 | round: 0 }}px;
    padding-left: {{ padding_horizontal_mobile }}rem;
    padding-right: {{ padding_horizontal_mobile }}rem;
    position: relative;
  }

  .product-tabs-tab-{{ section.id }} {
    padding: {{ tab_padding_vertical_mobile }}px {{ tab_padding_horizontal_mobile }}px;
    border-radius: {{ tab_radius }}px;
    background-color: {{ tab_bg_color }};
    border: {{ tab_border_thickness }}px solid {{ tab_border_color }};
    width: 100%;
    position: relative;
  }

  .product-tabs-tab-{{ section.id }}:not(:first-child) {
    margin-top: {{ tab_mt_mobile }}px;
  }

  .product-tabs-toggle-{{ section.id }} {
    display: flex;
    align-items: center;
    justify-content: space-between;
    cursor: pointer;
    gap: 12px;
  }

  .product-tabs-arrow-{{ section.id }} {
    width: {{ arrow_size_mobile }}px;
    height: {{ arrow_size_mobile }}px;
    flex-shrink: 0;
  }

  .product-tabs-arrow-{{ section.id }} svg {
    display: block;
    width: 100%;
    height: 100%;
    object-fit: cover;
    transition: all 0.25s ease 0s;
  }

  .product-tabs-arrow-{{ section.id }} svg path {
    fill: {{ arrow_color }};
  }

  .product-tabs-arrow-{{ section.id }}.active svg {
    transform: rotate(180deg);
    transition: all 0.25s ease 0s;
  }

  .product-tabs-question-wrapper-{{ section.id }} {
    display: flex;
    align-items: center;
    gap: {{ title_gap_mobile }}px;
  }

  .product-tabs-question-icon-{{ section.id }} {
    flex-shrink: 0;
    width: {{ title_icon_size_mobile }}px;
    height: {{ title_icon_size_mobile }}px;
  }

  .product-tabs-question-icon-{{ section.id }} img,
  .product-tabs-question-icon-{{ section.id }} svg {
    display: block;
    width: 100%; 
    height: 100%;
    object-fit: cover;
  }

  .product-tabs-question-icon-{{ section.id }} svg path {
    fill: {{ title_icon_color }};
  }

  .product-tabs-question-{{ section.id }} {
    margin: 0px;
    font-size: {{ title_size_mobile }}px;
    color: {{ title_color }};
    line-height: {{ title_height }}%;
    text-transform: unset;
    word-break: break-word;
    font-weight: 700;
  }

  .product-tabs-answer-{{ section.id }} {
    transition: all 0.2s ease-in-out;
    overflow: hidden;
  }

  .product-tabs-answer-{{ section.id }} *,
  .product-tabs-height-cal-{{ section.id }} *  {
    margin: 0px;
    font-size: {{ text_size_mobile }}px;
    color: {{ text_color }};
    line-height: {{ text_height }}%;
    text-transform: unset;
    word-break: break-word;
  }

  .product-tabs-answer-{{ section.id }} p,
  .product-tabs-height-cal-{{ section.id }} p {
    min-height: 10px;
  }

  .product-tabs-answer-{{ section.id }} p:not(:first-child ),
  .product-tabs-height-cal-{{ section.id }} p:not(:first-child ) {
    margin-top: 10px;
  } 

  .product-tabs-height-cal-{{ section.id }} {
    visibility: hidden;
    opacity: 0;
    pointer-events: none;
    position: absolute;
    top: 0;
    left: 0;
    width: calc(100% - ({{ tab_padding_horizontal_mobile | times: 2 }}px + {{ tab_border_thickness }}px));
    padding-top: {{ text_padding_top }}px;
  }
  
  @media(min-width: 1024px) {

    .section-{{ section.id }} {
      margin-top: {{ margin_top }}px !important;
      margin-bottom: {{ margin_bottom }}px !important;
    }
    
    .section-{{ section.id }}-settings {
      padding: 0 5rem;
      padding-top: {{ padding_top }}px;
      padding-bottom: {{ padding_bottom }}px;
      padding-left: {{ padding_horizontal }}rem;
      padding-right: {{ padding_horizontal }}rem;
    }

    .product-tabs-tab-{{ section.id }} {
      padding: {{ tab_padding_vertical }}px {{ tab_padding_horizontal }}px;
    }

    .product-tabs-tab-{{ section.id }}:not(:first-child) {
      margin-top: {{ tab_mt }}px;
    }
  
    .product-tabs-arrow-{{ section.id }} {
      width: {{ arrow_size }}px;
      height: {{ arrow_size }}px;
    }

    .product-tabs-question-wrapper-{{ section.id }} {
      gap: {{ title_gap }}px;
    }

    .product-tabs-question-icon-{{ section.id }} {
      width: {{ title_icon_size }}px;
      height: {{ title_icon_size }}px;
    }
    
    .product-tabs-question-{{ section.id }} {
      font-size: {{ title_size }}px;
    }

    .product-tabs-answer-{{ section.id }} *,
    .product-tabs-height-cal-{{ section.id }} * {
      font-size: {{ text_size }}px;      
    }

    .product-tabs-answer-{{ section.id }} {
      width: calc(100% - {{ tab_border_thickness | times: 2 }}px);
    }

    .product-tabs-height-cal-{{ section.id }} {
      width: calc(100% - ({{ tab_padding_horizontal | times: 2 }}px + {{ tab_border_thickness | times: 2 }}px));
    }
  }
  
{%- endstyle -%}

{% if title_custom %}
  <style>
    .product-tabs-question-{{ section.id }} {
      font-family: {{ title_font.family }}, {{ title_font.fallback_families }};
      font-weight: {{ title_font.weight }};
      font-style: {{ title_font.style }};
    }
  </style>
{% endif %}

{% if text_custom %}
  <style>
    .product-tabs-answer-{{ section.id }} * {
      font-family: {{ text_font.family }}, {{ text_font.fallback_families }};
      font-weight: {{ text_font.weight }};
      font-style: {{ text_font.style }};
    }

    .product-tabs-height-cal-{{ section.id }} * {
      font-family: {{ text_font.family }}, {{ text_font.fallback_families }};
      font-weight: {{ text_font.weight }};
      font-style: {{ text_font.style }};
    }
  </style>
{% endif %}

<script>
  (function () {
    function moreLoadProductForm() {
      var productForm = document.querySelector('#ss-product-tabs');
      if (!productForm){
        var productForm = document.querySelector('{% if tabs_position == "below_title" or tabs_position == "above_title" %}.product__title{% elsif tabs_position == "below_description" or tabs_position == "above_description" %}.product__description{% elsif tabs_position == "below_price" %}.price.price--large.price--show-badge{% else %}.button--add-to-cart, .product .product-form, .product-single__add-to-cart, .shopify-payment-button, .shopify-payment-button__button, .ProductForm__BuyButtons, .add-to-cart{% endif %}');
        if (!productForm) return;
        }
        const templateContent = document.querySelector(
          '#ss-product-tabs-{{ section.id }}'
        ).content;

        productForm.insertAdjacentElement(
          '{% if tabs_position == "below_buy" or tabs_position == "below_title" or tabs_position == "below_description" or tabs_position == "below_price" %}afterend{% else %}beforebegin{% endif %}',
          templateContent.firstElementChild
        );      
    }
    window.addEventListener('DOMContentLoaded', moreLoadProductForm);
  })();
</script>

